@model IEnumerable<EmployeeManagement.Models.Employee>
    <html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <title></title>
    </head>
    <body>
        <table>
            <thead>
                <tr>
                    <td>Id</td>
                    <td>Name</td>
                    <td>Department</td>
                </tr>
            </thead>
            <tbody>
                @foreach(var employee in Model)
                {
                <tr>
                    <td>
                        @employee.Id
                    </td>
                    <td>
                        @employee.Name
                    </td>
                    <td>
                        @employee.Department
                    </td>
                </tr>
                }
            </tbody>
        </table>
    </body>
    </html>