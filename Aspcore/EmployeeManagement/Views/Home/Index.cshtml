@model IEnumerable<Employee>
@{
    ViewBag.Title= "Employee List";
}
<table>
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Department</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>
                    @employee.Id
                </td>
                <td>
                    @employee.Name
                </td>
                <td>
                    @employee.Department
                </td>
            </tr>
        }
    </tbody>
</table>
